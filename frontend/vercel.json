{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/styles/(.*)",
            "dest": "/styles/$1"
        },
        {
            "src": "/scripts/(.*)",
            "dest": "/scripts/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "https://your-render-app.onrender.com"
    }
} 